{
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "require": {
    "ext-pdo": "*",
    "ext-redis": "*",
    "beberlei/doctrineextensions": "dev-master",
    "doctrine/dbal": "^3.4",
    "doctrine/migrations": "^3.5",
    "doctrine/orm": "^2.13",
    "fakerphp/faker": "^1.23",
    "itsgoingd/clockwork": "^5.2",
    "league/flysystem": "^3.0",
    "php-di/php-di": "^6.4",
    "psr/simple-cache": "^3.0",
    "slim/csrf": "^1.5",
    "slim/psr7": "^1.5",
    "slim/slim": "^4.10",
    "slim/twig-view": "^3.3",
    "symfony/cache": "^6.1",
    "symfony/console": "^6.1",
    "symfony/mailer": "^7.1",
    "symfony/rate-limiter": "^6.4",
    "symfony/twig-bridge": "^6.1",
    "symfony/webpack-encore-bundle": "^1.15",
    "twig/intl-extra": "^3.4",
    "twig/twig": "^3.4",
    "vlucas/phpdotenv": "^5.4",
    "vlucas/valitron": "^1.4"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.6",
    "phpunit/phpunit": "^11.4.1"
  }
}
